  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#5837a0"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#006d00"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#b20031"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#b20031"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#00003f"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#8b2800"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "background": "#b20031",
        "foreground": "#ffffff"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": "string source",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#00003f"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#00003f"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#006d00"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#8b2800"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#006d00"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#24292e"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#ffffff",
        "foreground": "#4a0000"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#ffffff",
        "foreground": "#006d00"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#ffffc3",
        "foreground": "#8b2800"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#f6f8fa",
        "background": "#003499"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#5837a0"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#003499"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#586069"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#4a0000"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#00003f"
      }
    }
  ],
  "semanticHighlighting": true
}
